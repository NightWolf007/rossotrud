- var nav = {'Главная': 'index.html', 'Внутренняя': 'inner.html'}

nav.navbar.navbar-default
	.navbar-header
		button(type="button" data-toggle="collapse" data-target="#navbar-collapsed" aria-expanded="false").navbar-toggle.collapsed
			span.icon-bar
			span.icon-bar
			span.icon-bar
			span.sr-only Toggle navigation

		a(href="./").logo
			img(src="./img/icons/rs-logo.svg", onerror="this.onerror=null; this.src='./img/icons/rs-logo.png'")

		a(href="javascript:;").navbar-brand Россотрудничество

		a(href="search.html").search
			img(src="./img/icons/ic-search.svg", onerror="this.onerror=null; this.src='./img/icons/ic-search.png'")

	.collapse.navbar-collapse#navbar-collapsed
		ul.nav.navbar-nav
			each linkPath, linkName in nav
				li: a(href=linkPath) #{linkName}